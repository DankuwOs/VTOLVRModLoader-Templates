{
  "author": "Danku",
  "name": "Vtol Mod Template",
  "description": "A template project doing template stuff.",
  "identity": "VtolVRModLoaderTemplate",
  "shortName": "vtolmodtemplate",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "VtolModTemplate",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net481"
        },
        {
          "choice": "net472"
        },
        {
          "choice": "net462"
        },
        {
          "choice": "netstandard2.0"
        }
      ],
      "defaultValue": "netstandard2.0"
    },
    "HarmonyID":
    {
      "type": "parameter",
      "defaultValue": "name.modname",
      "replaces": "name.modname"
    },
    "VTOLVRPath":
    {
      "type": "parameter",
      "defaultValue": "C:\\Program Files (x86)\\Steam\\steamapps\\common\\VTOL VR",
      "replaces": "C:\\Program Files (x86)\\Steam\\steamapps\\common\\VTOL VR"
    },
    "ModTitle":
    {
      "type": "parameter",
      "defaultValue": "Vtol Mod Title",
      "replaces": "VTOLMODTITLE"
    },
    "ModDescription":
    {
      "type": "parameter",
      "defaultValue": "Vtol Mod Description",
      "replaces": "VTOLMODDESCRIPTION"
    },
    "ModImageURL":
    {
      "type": "parameter",
      "defaultValue": "https://vtolvr-mods.com/static/img/Logo.png",
      "replaces": "VTOLMODIMAGE"
    }
  }
}
